{
    "directories": [
        "{HOME}\\audio"
    ],
    "muse_config": {
        "enable_preparation": true,
        "preparation_starts_minutes_from_end": 2,
        "preparation_starts_after_seconds_sleep": 10,
        "chance_speak_after_track": 0.2,
        "chance_speak_before_track": 0.3,
        "chance_speak_about_other_topics": 0.2
    },
    "prompts_directory": "prompts",
    "tongue_twisters_dir": "",
    "artists_file": "artists_example.json",
    "composers_file": "composers_example.json",
    "forms_file": "forms_example.json",
    "enable_dynamic_volume": true,
    "foreground_color": "white",
    "background_color": "#2596BE",
    "open_weather_api_key": "<KEY>",
    "open_weather_city": "Washington",
    "news_api_key": "<KEY>",
    "news_api_source_trustworthiness": {
        "bbc-news": 0.5
    },
    "muse_language_learning_language": "German",
    "coqui_tts_location": "{HOME}\\TTS-dev",
